
def server = Artifactory.server 'Artifactory'
pipeline {
  
  agent any
  
  environment {
  LIQUIBASE_COMMAND_CHANGELOG_FILE="changelog.sql"
  }
  stages {
    stage('upload-artifact'){
    steps{
      script {
    def uploadSpec = """{
        "files": [
         {
            "target": "ind-test/",
            "pattern": "$LIQUIBASE_COMMAND_CHANGELOG_FILE"
         }
        ]
     }"""
    server.upload spec: uploadSpec 
    }
   }
  }
 }
}



