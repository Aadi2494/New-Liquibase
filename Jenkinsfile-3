
def server = Artifactory.server 'Artifactory'
pipeline {
  
  agent any
  
  
  stages {
    stage('upload-artifact'){
    steps{
      script {
    def uploadSpec = """{
        "files": [
         {
            "target": "ind-test/"
            "pattern": changelog.sql
         }
        ]
     }"""
    server.upload spec: uploadSpec 
    }
   }
  }
 }
}



